sqoop import 
--options-file connection.props \
--table products \
--target-dir /user/bbastola/Dataset/products_parquet \
--delete-target-dir \
--as-parquetfile \
-m 1

To copy all /parquet file from hdfs to local:
hadoop fs -get /user/bbastola/Dataset/products_parquet/*.parquet

#To get the meta from the parquet file

parquet-tools meta .parquet_filename

parquet-tools schema .parquet_filename 


To convert Parquet to Json
parquet-tools cat -j .parquet_filename













