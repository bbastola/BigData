select distince(order_status) from orders;

---> output is:
CANCELED
CLOSED
COMPLETE
ON_HOLD
PAYMENT_REVIEW
PENDING
PENDING_PAYMENT
PROCESSING
SUSPECTED_FRAUD

select order_status,
       case
       when order_status in ('CLOSED', 'COMPLETE') then 'order complete'
       when order_status in ('ON_HOLD', 'PENDING', 'PROCESSING') then 'order not completed'
       else 'RISKY' 
       end from orders limit 20;

select nvl(order_status, 'status missing') from orders limit 10;
--> this will print all orders_status from table orders AND if any field in order_status in null,
it will repalce that with status missing.
