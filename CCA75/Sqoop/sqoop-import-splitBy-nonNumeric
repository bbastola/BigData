//if split-by has to use non-numeric column then we have to add additional parameter
// -Dorg.apache.sqoop.splitter.allow_text_splitter=true 

sqoop import \
-Dorg.apache.sqoop.splitter.allow_text_splitter=true \
--options-file ./connection.props \
--table orders \
--warehouse-dir /user/bbastola/sqoop_import/ \
--split-by order_status \
--num-mappers 6 