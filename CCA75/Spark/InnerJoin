orders = sc.textFile("/public/retail_db/orders/part-00000")
ordersMap = orders.map(lambda rec: (int(rec.split(",")[0]),rec.split(",")[1]))

orderItems = sc.textFile("/public/retail_db/order_items/part-00000")
orderItemsMap = orderItems.map(lambda rec: (int(rec.split(",")[1]),float(rec.split(",")[4])))


orderJoin = ordersMap.join(orderItemsMap)

for i in orderJoin.take(10): print i
